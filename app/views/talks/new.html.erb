<%= form_for @talk do |f| %>
  <%= f.label :topic %>
  <%= f.text_field :topic %>

  <%= f.label :date %>
  <%= f.date_field(:date)%>

  <%= f.label :video_url, 'Video URL' %>
  <%= f.text_field :video_url %>

  <%= f.label :description %>
  <%= f.text_area :description %>

  <%= f.submit 'Add Talk' %>
<%-end-%>
